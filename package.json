{
  "name": "wxb-uniapp-collect-sdk",
  "version": "v0.0.11",
  "description": "用于uniapp项目埋点",
  "author": "刘胜",
  "license": "ISC",
  "homepage": "http://gitlab.wangxiaobao.com/npm/wxb-uniapp-collect-sdk",
  "repository": {
    "type": "git",
    "url": "http://gitlab.wangxiaobao.com/npm/wxb-uniapp-collect-sdk"
  },
  "keywords": [],
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "publishConfig": {
    "registry": "http://nexus3.wangxiaobao.com/repository/wxp-private/"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "dev": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "build": "tsc && tsc-alias && npm run cp-types",
    "release": "tsc && npm publish",
    "cp-types": "copyfiles src/types/* bin",
    "eslint": "eslint . --fix --ext .ts",
    "prepare": "husky install",
    "vue2-wx": "cd examples/uniapp-vue2 && npm run dev:mp-weixin",
    "vue2-tt": "cd examples/uniapp-vue2 && npm run dev:mp-toutiao"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@dcloudio/types": "^3.4.3",
    "@types/uni-app": "^1.4.7",
    "@types/wechat-miniprogram": "^3.1.0",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.26.0",
    "eslint-config-wxb-typescript": "^0.0.18",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tslint-config-prettier": "^1.18.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --ext .ts"
    ]
  }
}
